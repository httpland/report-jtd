{
  "metadata": {
    "description": "Content security policy violation report body."
  },
  "properties": {
    "documentURL": {
      "metadata": {
        "description": "Violation's URL."
      },
      "type": "string"
    },
    "referrer": {
      "metadata": {
        "description": "Violation's referrer."
      },
      "type": "string",
      "nullable": true
    },
    "blockedURL": {
      "metadata": {
        "description": "Violation's resource."
      },
      "type": "string",
      "nullable": true
    },
    "effectiveDirective": {
      "metadata": {
        "description": "Violation's effective directive."
      },
      "type": "string"
    },
    "originalPolicy": {
      "metadata": {
        "description": "The serialization of violation's policy."
      },
      "type": "string"
    },
    "sourceFile": { "type": "string", "nullable": true },
    "sample": {
      "metadata": {
        "description": "Violation's sample"
      },
      "type": "string",
      "nullable": true
    },
    "disposition": {
      "metadata": {
        "description": "The disposition of violation's policy."
      },
      "enum": ["enforce", "report"]
    },
    "statusCode": {
      "metadata": { "description": "Violation's status." },
      "type": "uint16"
    },
    "lineNumber": {
      "metadata": {
        "description": "Violation's line number"
      },
      "type": "uint32",
      "nullable": true
    },
    "columnNumber": {
      "metadata": {
        "description": "Violation's column number"
      },
      "type": "uint32",
      "nullable": true
    }
  }
}
